{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/auth/callback", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}